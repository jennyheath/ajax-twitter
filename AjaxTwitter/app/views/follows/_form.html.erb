<%# if current_user.follows?(user) %>
  <%#= button_to "Unfollow", user_follow_url(user), method: :delete, class: "follow-toggle" %>
  <%#= puts user %>
<%# else %>
  <%#= button_to "Follow", user_follow_url(user), method: :post, class: "follow-toggle"  %>
<%# end %>

<button class="follow-toggle" data-user-id="<%= user.id %>"
        data-initial-follow-state=
          "<%= (current_user.follows?(user) ? "followed" : "unfollowed") %>">
</button>
<br>
