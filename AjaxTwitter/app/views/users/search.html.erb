<h1>Search!</h1>

<div class="users-search">
  <input type="text" name="query" placeholder="search users...">
  <ul class="users">
  </ul>
</div>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user_url(user) %>
      <%= render "follows/form", user: user %>
    </li>
  <% end %>
</ul>
